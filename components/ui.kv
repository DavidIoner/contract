<App@MDFloatLayout>

    MDTextField:
        id: licensee
        hint_text: "Licensee name"
        pos_hint: {"x": 0.05, "y": 0.9}
        size_hint_x: 0.4
    
    MDTextField:
        id: company
        hint_text: "Company name"
        pos_hint: {"x": 0.05, "y": 0.8}
        size_hint_x: 0.4

    MDTextField:
        id: located_at
        hint_text: "Located at"
        pos_hint: {"x": 0.05, "y": 0.7}
        size_hint_x: 0.4

    MDTextField:
        id: phone
        hint_text: "Phone number"
        pos_hint: {"x": 0.05, "y": 0.6}
        size_hint_x: 0.4

    MDTextField:
        id: location
        hint_text: "Location (city, country)"
        pos_hint: {"x": 0.05, "y": 0.5}
        size_hint_x: 0.4

    MDTextField:
        id: start_date
        hint_text: "start_date (dd/mm/yyyy)"
        pos_hint: {"x": 0.05, "y": 0.4}
        size_hint_x: 0.4

    MDTextField:
        id: end_date
        hint_text: "end_date (dd/mm/yyy)"
        pos_hint: {"x": 0.05, "y": 0.3}
        size_hint_x: 0.4

    MDTextField:
        id: licensor
        hint_text: "Licensor name"
        pos_hint: {"x": 0.05, "y": 0.2}
        size_hint_x: 0.4

    MDTextField:
        id: onboarding
        hint_text: "Onboarding deposit (USD)"
        pos_hint: {"x": 0.55, "y": 0.9}
        size_hint_x: 0.4

    MDTextField:
        id: security_MXN
        hint_text: "Security deposit (MXN)"
        pos_hint: {"x": 0.55, "y": 0.8}
        size_hint_x: 0.4

    MDTextField:
        id: apartment_price_MXN
        hint_text: "apartment fee (MXN)"
        pos_hint: {"x": 0.55, "y": 0.7}
        size_hint_x: 0.4

    MDTextField:
        id: wage_MXN
        hint_text: "Wage (MXN)"
        pos_hint: {"x": 0.55, "y": 0.6}
        size_hint_x: 0.4

    MDTextField:
        id: christmas_MXN
        hint_text: "Christmas bonus (MXN)"
        pos_hint: {"x": 0.55, "y": 0.5}
        size_hint_x: 0.4   



    MDRaisedButton:
        id: btn
        text: "Submit"
        pos_hint: {"x": 0.55, "y": 0.2}
        size_hint: 0.2, 0.1
        on_release: 
            app.submit()

    MDLabel:
        id: label
        text: ""
        pos_hint: {"x": 0.55, "y": 0.1}
        size_hint: 0.4, 0.1

App